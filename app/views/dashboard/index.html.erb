<% if current_user.type == "Recruiter" %>
  <%= render partial: 'dashboard/recruiter' %>
<% elsif current_user.type == 'Player' %>
  <%= render partial: 'dashboard/player' %>
<% elsif current_user.type == "Coach" %>
  <%= render partial: 'dashboard/coach' %>
<% else %>
  <%= render partial: 'dashboard/admin' %>
<% end %>
